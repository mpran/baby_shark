{
  "manifest_version": 3,
  "name": "Baby Shark",
  "description": "Extension for Digital Ocean searches",
  "version": "1.0.7.0",
  "version_name": "1.0.7 stable",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/baby-shark-16.png",
      "32": "icons/baby-shark-32.png",
      "48": "icons/baby-shark-48.png",
      "96": "icons/baby-shark-96.png",
      "128": "icons/baby-shark-128.png"
    }
  },
  "permissions": [
    "storage",
    "cookies",
    "webRequest"
  ],
  "host_permissions": ["https://cloud.digitalocean.com/"]
}
